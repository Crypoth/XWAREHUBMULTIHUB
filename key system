-- Key Input Box
local KeyBox = Instance.new("TextBox")
KeyBox.Size = UDim2.new(0, 350, 0, 45)
KeyBox.Position = UDim2.new(0.5, -175, 0, 110)
KeyBox.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
KeyBox.PlaceholderText = "Enter Key Here!"  -- Placeholder text
KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
KeyBox.TextSize = 20
KeyBox.Font = Enum.Font.GothamBold
KeyBox.ClearTextOnFocus = true
KeyBox.Parent = KeyFrame
KeyBox.ZIndex = 3

-- Remove any potential "TextBox" or other conflicting elements
Instance.new("UICorner", KeyBox).CornerRadius = UDim.new(0, 10)
Instance.new("UIStroke", KeyBox).Color = Color3.fromRGB(80, 80, 100)

-- Submit Key Button
local SubmitButton = Instance.new("TextButton")
SubmitButton.Size = UDim2.new(0, 160, 0, 45)
SubmitButton.Position = UDim2.new(0.5, -170, 0, 180)
SubmitButton.Text = "Unlock"
SubmitButton.Font = Enum.Font.GothamBold
SubmitButton.TextSize = 18
SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
SubmitButton.BackgroundColor3 = Color3.fromRGB(90, 70, 180)
SubmitButton.Parent = KeyFrame
SubmitButton.ZIndex = 3

Instance.new("UICorner", SubmitButton).CornerRadius = UDim.new(0, 10)
Instance.new("UIStroke", SubmitButton).Color = Color3.fromRGB(120, 100, 200)

-- Get Key Button
local GetKeyButton = Instance.new("TextButton")
GetKeyButton.Size = UDim2.new(0, 160, 0, 45)
GetKeyButton.Position = UDim2.new(0.5, 10, 0, 180)
GetKeyButton.Text = "Get Key"
GetKeyButton.Font = Enum.Font.GothamBold
