-- Static Text
local StaticText = Instance.new("TextLabel")
StaticText.Size = UDim2.new(1, -20, 1, -35)
StaticText.Position = UDim2.new(0, 10, 0, 30)
StaticText.BackgroundTransparency = 1
StaticText.Text = "The key is very simple and quick to retrieve, all you have to do is join the discord."
