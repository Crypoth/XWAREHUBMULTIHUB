local Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/main/source.lua", true))()

local KeyGui = Instance.new("ScreenGui")
KeyGui.Name = "XWARE_KeySystem"
KeyGui.ResetOnSpawn = false
KeyGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
KeyGui.Parent = game:GetService("CoreGui")

local KeyFrame = Instance.new("Frame")
KeyFrame.Name = "KeyFrame"
KeyFrame.Size = UDim2.new(0, 500, 0, 450)
KeyFrame.Position = UDim2.new(0.5, -250, 0.5, -225)
KeyFrame.AnchorPoint = Vector2.new(0, 0)
KeyFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
KeyFrame.BackgroundTransparency = 0.15
KeyFrame.Parent = KeyGui
KeyFrame.Active = true
KeyFrame.Draggable = true
KeyFrame.ZIndex = 2

-- Round frame corners
local Corner = Instance.new("UICorner", KeyFrame)
Corner.CornerRadius = UDim.new(0, 14)

-- Thicker flower-like outline effect
local OutlineFrame = Instance.new("Frame")
OutlineFrame.Size = UDim2.new(1, 20, 1, 20)  -- Increased size for a thicker outline
OutlineFrame.Position = UDim2.new(0, -10, 0, -10)  -- Adjusted position to match thicker outline
OutlineFrame.BackgroundTransparency = 1
OutlineFrame.Parent = KeyFrame

-- Create flower-like effect with UIGradient
local FlowerOutline = Instance.new("UIGradient", OutlineFrame)
FlowerOutline.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(120, 100, 200)),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(80, 60, 120)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(120, 80, 180))
})
FlowerOutline.Rotation = 45  -- Rotated to simulate the flower-like effect

-- Title
local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Position = UDim2.new(0, 0, 0, 10)
Title.BackgroundTransparency = 1
Title.Text = "XWARE HUB â€” KEY SYSTEM"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.GothamBold
