-- Add "X" button to close the GUI
local CloseButton = Instance.new("TextButton")
CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Position = UDim2.new(1, -40, 0, 10)
CloseButton.Text = "X"
CloseButton.Font = Enum.Font.GothamBold
CloseButton.TextSize = 20
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
CloseButton.Parent = KeyFrame
CloseButton.ZIndex = 4

CloseButton.MouseButton1Click:Connect(function()
    -- Unload with a smooth animation before destroying
    KeyFrame:TweenSize(UDim2.new(0, 500, 0, 450), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.5, true)
    wait(0.5)
    KeyGui:Destroy()
end)

-- Button Hover Effects with animation
SubmitButton.MouseEnter:Connect(function()
    SubmitButton:TweenSize(UDim2.new(0, 170, 0, 50), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.2, true)
end)

SubmitButton.MouseLeave:Connect(function()
    SubmitButton:TweenSize(UDim2.new(0, 160, 0, 45), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.2, true)
end)

GetKeyButton.MouseEnter:Connect(function()
    GetKeyButton:TweenSize(UDim2.new(0, 180, 0, 50), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.2, true)
end)

GetKeyButton.MouseLeave:Connect(function()
    GetKeyButton:TweenSize(UDim2.new(0, 160, 0, 45), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.2, true)
end)

-- Add effects when the GUI is loaded/unloaded
KeyFrame:TweenSize(UDim2.new(0, 500, 0, 450), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.5, true)  -- Smooth animation for loading
wait(1)
KeyFrame:TweenSize(UDim2.new(0, 500, 0, 450), Enum.EasingDirection.Out, Enum.EasingStyle.Sine, 0.5, true)
-- Existing SubmitButton functionality
GetKeyButton.MouseButton1Click:Connect(function()
    if setclipboard then
        local success, err = pcall(function()
            setclipboard("https://discord.gg/EZvYf4ntmK")
        end)

        if success then
            Luna:Notification({
                Title = "Link Copied!",
                Content = "The Discord invite link has been copied to your clipboard.",
                Icon = "link",
                ImageSource = "Material",
                Duration = 5
            })
        else
            Luna:Notification({
                Title = "Copy Failed",
                Content = "Failed to copy the invite link. Please try again.",
                Icon = "error",
                ImageSource = "Material",
                Duration = 5
            })
            warn("Clipboard error: " .. tostring(err))
        end
    else
        Luna:Notification({
            Title = "Clipboard Unsupported",
            Content = "Your exploit does not support clipboard functionality.",
            Icon = "error",
            ImageSource = "Material",
            Duration = 5
        })
        warn("Clipboard function 'setclipboard' not available.")
    end
end)

-- Define your correct key
local CorrectKey = "777"

SubmitButton.MouseButton1Click:Connect(function()
    local enteredKey = KeyBox.Text

    if enteredKey == CorrectKey then
        StatusLabel.Text = "Status: Key Verified!"
        StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)

        Luna:Notification({
            Title = "Access Granted!",
            Content = "You have successfully unlocked the hub.",
            Icon = "check_circle",
            ImageSource = "Material",
            Duration = 4
        })

        wait(1)

        -- Optional: animate GUI closing
        KeyFrame:TweenSize(UDim2.new(0, 500, 0, 0), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.4, true)
        wait(0.5)
        KeyGui:Destroy()

        -- Load your actual hub script
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Crypoth/SCRIPT-SCRIPT/refs/heads/main/here"))()
    else
        StatusLabel.Text = "Status: Invalid Key. Please try again."
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)

        Luna:Notification({
            Title = "Invalid Key",
            Content = "The key you entered is incorrect.",
            Icon = "error",
            ImageSource = "Material",
            Duration = 5
        })
    end
end)
