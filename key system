-- Static Text Box-like Container (Under Toggle Buttons)
local StaticTextBox = Instance.new("Frame")
StaticTextBox.Size = UDim2.new(0, 460, 0, 100)
StaticTextBox.Position = UDim2.new(0.5, -230, 0, 290)
StaticTextBox.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
StaticTextBox.BorderSizePixel = 2
StaticTextBox.BorderColor3 = Color3.fromRGB(80, 80, 100)
StaticTextBox.Parent = KeyFrame
StaticTextBox.ZIndex = 3

local CornerStatic = Instance.new("UICorner")
CornerStatic.CornerRadius = UDim.new(0, 10)
CornerStatic.Parent = StaticTextBox

-- "READ ME!" Title
local ReadMeTitle = Instance.new("TextLabel")
ReadMeTitle.Size = UDim2.new(1, -20, 0, 30)
ReadMeTitle.Position = UDim2.new(0, 10, 0, 8)
ReadMeTitle.BackgroundTransparency = 1
